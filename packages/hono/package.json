{
  "name": "@agent-seo/hono",
  "version": "1.0.1",
  "description": "Hono middleware for AI-readable websites",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pontiggia/agent-seo",
    "directory": "packages/hono"
  },
  "homepage": "https://github.com/pontiggia/agent-seo#readme",
  "bugs": "https://github.com/pontiggia/agent-seo/issues",
  "keywords": ["seo", "ai", "llm", "hono", "middleware", "llms-txt", "agent", "bot"],
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agent-seo/core": "workspace:*"
  },
  "peerDependencies": {
    "hono": "^4.0.0"
  },
  "devDependencies": {
    "hono": "^4.0.0"
  }
}
