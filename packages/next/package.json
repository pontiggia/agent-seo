{
  "name": "@agent-seo/next",
  "version": "1.0.3",
  "description": "Next.js plugin for AI-readable websites",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pontiggia/agent-seo",
    "directory": "packages/next"
  },
  "homepage": "https://github.com/pontiggia/agent-seo#readme",
  "bugs": "https://github.com/pontiggia/agent-seo/issues",
  "keywords": [
    "seo",
    "ai",
    "llm",
    "nextjs",
    "next",
    "middleware",
    "llms-txt",
    "agent",
    "bot"
  ],
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./middleware": {
      "import": {
        "types": "./dist/middleware.d.ts",
        "default": "./dist/middleware.js"
      },
      "require": {
        "types": "./dist/middleware.d.cts",
        "default": "./dist/middleware.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@agent-seo/core": "workspace:*"
  },
  "peerDependencies": {
    "next": "^14.0.0 || ^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "next": "^16.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
